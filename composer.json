{
  "name": "moodify/app",
  "description": "A mood-based Spotify playlist finder",
  "type": "project",
  "require": {
    "php": ">=7.4",
    "vlucas/phpdotenv": "^5.5",
    "guzzlehttp/guzzle": "^7.0",
    "ext-json": "*"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "minimum-stability": "stable"
}
